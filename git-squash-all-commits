#!/usr/bin/env bash

repo=$(git config remote.origin.url)

echo -e "[x] Copying origin URL: ${repo}"

echo -e "[x] Run: rm -rf .git/"
rm -rf .git/

echo -e "[x] Run: git setup"
echo -e "■■■■■■■■■■■■■■■■■■"
git setup

echo -e "[x] Run: git config remote.origin.url ${repo}\n"
git config remote.origin.url ${repo}
